<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zfh.shop.product.mapper.SpuBannerMapper">
    <select id="getSpuBannerListBySpuId" resultType="com.zfh.shop.product.bean.po.SpuBanner">
        select * from `spu_banner` where spu_banner_spu_id = #{spu_id}
    </select>
</mapper>